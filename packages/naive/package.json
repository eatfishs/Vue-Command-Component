{
  "name": "@vue-cmd/naive",
  "version": "1.0.6",
  "type": "module",
  "description": "Naive UI命令式组件适配器 - 让Naive UI的Modal、Drawer等组件支持命令式调用",
  "homepage": "https://pandavips.github.io/Vue-Command-Component/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandavips/Vue-Command-Component.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "naive-ui",
    "naiveui",
    "command",
    "component",
    "modal",
    "drawer",
    "dialog",
    "popup",
    "弹窗",
    "抽屉",
    "命令式",
    "imperative",
    "n-modal",
    "n-drawer",
    "promise",
    "async",
    "adapter"
  ],
  "license": "MIT",
  "author": "pandavips",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "dependencies": {},
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vue-tsc": "^2.0.0"
  },
  "peerDependencies": {
    "naive-ui": "2.x",
    "vue": "3.x",
    "@vue-cmd/core": "workspace:*"
  }
}
