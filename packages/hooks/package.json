    "dev": "vite build --watch",
    "publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "@vue-cmd/core": "workspace:*",
    "vue-router": "^4.0.0",
    "vue": "^3.0.0"
