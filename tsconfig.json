{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "config/**/*",
    "dist/**/*",
    "./typed-router.d.ts",
    "vite.config.*"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "target": "ES2016",
    "skipLibCheck": true,
    "composite": true,
    "noEmit": true,
    "module": "ESNext",
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "jsx": "preserve",
    "types": [
      "vite/client",
      "unplugin-vue-router/client",
      "node",
    ],
  }
}
