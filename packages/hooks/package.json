{
  "name": "@vue-cmd/hooks",
  "version": "0.2.1",
  "type": "module",
  "description": "Vue Command Component hooks",
  "author": "pandavips",
  "license": "MIT",
  "keywords": [
    "vue",
    "vue3",
    "component",
    "hooks",
    "command",
    "dialog"
  ],
  "main": "dist/index.cjs.js",
