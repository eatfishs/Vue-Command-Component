{
  "name": "@vue-cmd/core",
  "version": "1.1.0",
  "type": "module",
  "description": "Vue Command Component Core Library",
  "homepage": "https://pandavips.github.io/Vue-Command-Component/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandavips/Vue-Command-Component.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "command",
    "component",
    "core"
  ],
  "license": "MIT",
  "author": "pandavips",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "dependencies": {
  },
  "devDependencies": {},
  "peerDependencies": {
    "vue": "3.x"
  }
}
