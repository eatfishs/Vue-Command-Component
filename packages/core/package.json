{
  "name": "@vue-cmd/core",
  "version": "1.1.2",
  "type": "module",
  "description": "Vue3命令式组件核心库 - 简化弹窗、对话框等组件的调用方式，告别繁琐的状态管理",
  "homepage": "https://pandavips.github.io/Vue-Command-Component/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandavips/Vue-Command-Component.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "command",
    "component",
    "dialog",
    "modal",
    "popup",
    "弹窗",
    "对话框",
    "命令式",
    "imperative",
    "functional",
    "promise",
    "async",
    "state-management",
    "ui-library",
    "typescript"
  ],
  "license": "MIT",
  "author": "pandavips",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "dependencies": {},
  "devDependencies": {},
  "peerDependencies": {
    "vue": "3.x"
  }
}
