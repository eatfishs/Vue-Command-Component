{
  "name": "vue-command-component-monorepo",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build:watch": "pnpm --filter '@vue-cmd/*' build --watch",
    "build": "pnpm --filter '@vue-cmd/*' build",
    "build:core": "pnpm --filter '@vue-cmd/core' build",
    "build:element-plus": "pnpm --filter '@vue-cmd/element-plus' build",
    "build:vant": "pnpm --filter '@vue-cmd/vant' build",
    "build:naive": "pnpm --filter '@vue-cmd/naive' build",
    "doc:build": "vitepress build packages/docs",
    "doc:dev": "vitepress dev packages/docs --port 3000",
    "doc:preview": "vitepress preview packages/docs --port 3000"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vue-cmd/core": "workspace:*",
    "@vue-cmd/element-plus": "workspace:*",
    "@vue-cmd/vant": "workspace:*",
    "@vue-cmd/naive": "workspace:*",
    "build": "^0.1.4",
    "viewerjs": "^1.11.7",
    "vitepress-plugin-image-viewer": "^1.1.6",
    "vant": "~4.9.7",
    "element-plus": "~2.9.9",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@shikijs/vitepress-twoslash": "^3.3.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/tsconfig": "^0.5.1",
    "sass": "^1.79.4",
    "semantic-release": "^21.0.1",
    "typescript": "^5.5.3",
    "unocss": "^66.1.3",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.2.2",
    "vite-plugin-libcss": "^1.1.1",
    "vitepress": "^1.6.3",
    "vitepress-demo-plugin": "^1.4.1",
    "vitepress-plugin-group-icons": "^1.5.2",
    "vue-tsc": "^2.0.29"
  }
}
